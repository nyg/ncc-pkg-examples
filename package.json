{
  "name": "ncc-pkg-examples",
  "version": "1.0.0",
  "bin": "server.js",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist-*",
    "pkg": "pkg .next/standalone --public --public-packages \"*\" --no-bytecode --compress=GZip"
  },
  "dependencies": {
    "next": "12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "pkg": "5.5.2"
  },
  "pkg": {
    "assets": [
      ".next/**/*.*",
      ".next/**/*",
      "node_modules/**/*.*"
    ],
    "targets": [
      "node16-macos-x64"
    ],
    "outputPath": "dist-pkg"
  }
}
